{
  "name": "krmr-nestjs-boilerplate",
  "version": "1.1.1",
  "description": "KRMR",
  "author": "ilterugur",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "pnpm patch-modules",
    "prebuild": "rimraf dist && rimraf dist2",
    "build:first": "nest build",
    "build": "pnpm prebuild && pnpm build:first && node esbuild",
    "build:mails": "cd './mail-templates' && pnpm build",
    "orm": "mikro-orm-esm",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "node node_modules/vite/bin/vite.js",
    "dev:mails": "cd './mail-templates' && pnpm dev",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "patch-modules": "npx @milahu/patch-package",
    "nestia:sdk": "npx nestia sdk",
    "nestia:swagger": "npx nestia swagger",
    "nestia": "pnpm nestia:sdk && pnpm nestia:swagger"
  },
  "dependencies": {
    "@casl/ability": "6.1.1",
    "@codebrew/nestjs-storage": "^0.1.6",
    "@mikro-orm/core": "5.4.2",
    "@mikro-orm/knex": "5.4.2",
    "@mikro-orm/mysql": "5.4.2",
    "@mikro-orm/nestjs": "5.1.2",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "9.0.11",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "9.0.11",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.0.11",
    "@nestjs/schedule": "2.1.0",
    "@nestjs/swagger": "6.1.2",
    "@nestjs/throttler": "3.0.0",
    "@node-rs/argon2": "^1.1.1",
    "@slynova/flydrive": "^1.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "convert-pro": "^1.3.2",
    "cookie-parser": "^1.4.6",
    "cron": "^2.0.0",
    "date-fns": "^2.28.0",
    "decode-uri-component": "^0.4.1",
    "dotenv": "^16.0.1",
    "dotenv-mustache": "^1.0.0",
    "dotenv-parse-variables": "^2.0.0",
    "express": "^4.18.1",
    "express-handlebars": "^6.0.6",
    "file-type": "18.0.0",
    "flat": "^5.0.2",
    "fs-extra": "^10.1.0",
    "handlebars": "^4.7.7",
    "hashids": "^2.2.10",
    "helmet": "6.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.0.0",
    "libphonenumber-js": "^1.10.13",
    "lodash": "^4.17.21",
    "mikro-orm-soft-delete": "^0.1.0",
    "multer": "1.4.5-lts.1",
    "mysql": "^2.18.1",
    "nanoid": "^4.0.0",
    "nestia-helper": "^3.0.9",
    "nestjs-url-generator": "^1.0.1",
    "node-monkey": "^1.1.5",
    "nodemailer": "^6.7.8",
    "p-iteration": "^1.1.8",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "qs": "^6.11.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "stripe": "^12.7.0",
    "swagger-ui-express": "^4.4.0",
    "tiny-glob": "^0.2.9",
    "ts-deepmerge": "3.0.0",
    "typescript-json": "^3.1.1"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^5.4.2",
    "@mikro-orm/entity-generator": "^5.4.2",
    "@mikro-orm/migrations": "^5.4.2",
    "@mikro-orm/seeder": "^5.4.2",
    "@milahu/patch-package": "^6.4.14",
    "@nestjs/cli": "9.1.2",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.0.11",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.207",
    "@swc/helpers": "^0.4.3",
    "@swc/jest": "^0.2.21",
    "@swc/register": "^0.1.10",
    "@types/cookie-parser": "^1.4.3",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "29.0.0",
    "@types/lodash": "^4.14.182",
    "@types/multer": "^1.4.7",
    "@types/node": "18.7.15",
    "@types/nodemailer": "^6.4.5",
    "@types/passport-jwt": "^3.0.6",
    "@types/qs": "^6.9.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "esbuild": "0.15.7",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.0.2",
    "nestia": "^3.0.13",
    "prettier": "^2.3.2",
    "run-script-webpack-plugin": "^0.1.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "tslib": "^2.4.0",
    "ttypescript": "^1.5.13",
    "type-fest": "^2.13.0",
    "typescript": "^4.8.2",
    "vite": "3.1.0",
    "vite-plugin-node": "^1.0.0",
    "vite-tsconfig-paths": "^3.4.1",
    "webpack": "^5.74.0",
    "webpack-node-externals": "^3.0.0"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/config/mikro-orm.config.ts",
      "./dist/config/mikro-orm.config.js"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@slynova/flydrive-gcs",
        "@slynova/flydrive-s3",
        "express"
      ]
    }
  }
}
